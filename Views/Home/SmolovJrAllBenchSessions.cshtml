@model SmolovJrBench.Models.AllBenchSessionsModel;


<div class="workout_sessions_container">
    <div class="table_header">
        <div class="col">Session</div>
        <div class="col">Reps</div>
        <div class="col">Sets</div>
        <div class="col">Weight</div>
    </div>
    @for(int i = 0; i < Model.AllSessions.Count(); i++)
    {
        <div class="row">
            <div class="col">@(i + 1)</div>
            <div class="col">@Model.AllSessions[i].NrOfReps</div>
            <div class="col">@Model.AllSessions[i].NrOfSets</div>
            <div class="col">@Model.AllSessions[i].WorkingWeight kg</div>
        </div>
    }
</div>

